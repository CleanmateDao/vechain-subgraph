{
  "name": "vechain-subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "configure:testnet": "node scripts/configure.js vechain-testnet",
    "configure:mainnet": "node scripts/configure.js vechain-mainnet",
    "codegen": "graph codegen",
    "build": "graph build",
    "codegen:testnet": "yarn configure:testnet && yarn codegen",
    "codegen:mainnet": "yarn configure:mainnet && yarn codegen",
    "build:testnet": "yarn configure:testnet && yarn build",
    "build:mainnet": "yarn configure:mainnet && yarn build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ vechain-subgraph",
    "create-local": "graph create --node http://localhost:8020/ vechain-subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ vechain-subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 vechain-subgraph",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.97.1",
    "@graphprotocol/graph-ts": "0.37.0"
  },
  "devDependencies": { "matchstick-as": "0.6.0" }
}
